<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<!-- Remote names -->
	<remote name="thlg" 
		fetch="https://github.com/TeamHackLG/" />
	<remote name="tv" 
		fetch="https://github.com/TeamVee/" />
	<remote name="floppy"
		fetch="https://github.com/FloppyTeam/"
		
	<!-- Unofficial Builds -->
	<remove-project name="CyanogenMod/android_packages_apps_CMUpdater" />
	
	<!-- THLG Display CAF -->
	<remove-project name="CyanogenMod/android_hardware_qcom_display-caf" />
	<project path="hardware/qcom/display-caf"  name="android_hardware_qcom_display-caf"  revision="cm-11.0"            remote="floppy" />
	
	<!-- TWRP -->
	<project path="bootable/recovery-omni"     name="android_bootable_recovery"          revision="cm-11.0"            remote="tv" />
	
	<!-- Common Project -->
	<project path="kernel/lge/msm7x27a-common" name="lge-kernel-lproj"                   revision="android-msm-3.4-kk" remote="floppy" />
	<project path="vendor/lge"                 name="proprietary_vendor_lge"             revision="cm-11.0"            remote="floppy" />
	<project path="device/lge/msm7x27a-common" name="android_device_lge_msm7x27a-common" revision="cm-11.0"            remote="floppy" />
	
	<!-- Webkit -->
	<project path="external/webkit"            name="android_external_webkit"            revision="master"             remote="floppy" />
</manifest>
